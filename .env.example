# Postgres Database Configuration
# Copy this file to .env and update with your values

# Postgres Connection
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=netflix_analytics
POSTGRES_DATABASE=netflix_analytics

# dbt Configuration
DBT_TARGET=dev
DBT_DEV_SCHEMA=dev
DBT_PROD_SCHEMA=prod
DBT_DEV_THREADS=4
DBT_PROD_THREADS=8

# Docker Configuration
# For Docker, POSTGRES_HOST should be 'postgres' (the service name)
# For local development, POSTGRES_HOST should be 'localhost'
# POSTGRES_PORT in Docker: use 5432 (internal) or 5433 (external mapping)
